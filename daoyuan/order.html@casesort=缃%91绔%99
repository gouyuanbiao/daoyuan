<script>function check(obj) {
    $('#domain input').each(function () {
        if (this != obj)
            $(this).attr("checked", false);
        else {
            if ($(this).prop("checked"))
                $(this).attr("checked", true);
            else
                $(this).attr("checked", false);
        }
    });
}</script>
<script>function check1(obj) {
    $('#packaging input').each(function () {
        if (this != obj)
            $(this).attr("checked", false);
        else {
            if ($(this).prop("checked"))
                $(this).attr("checked", true);
            else
                $(this).attr("checked", false);
        }
    });
}</script>
<script>
 

function InputCheck(orderform)
{
  if (orderform.company.value == "")
  {
    alert("请输入项目名称!");
    orderform.company.focus();
    return (false);
  }
  if (orderform.content.value == "")
  {
    alert("请输入需求说明!");
    orderform.content.focus();
    return (false);
  }
  
}

 
</script>
